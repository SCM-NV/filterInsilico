{
    "$schema": "http://json-schema.org/draft-04/schema",
    "description": "Available Filters",
    "type": "object",
    "oneOf": [{
	"properties": {
	    "functional_group": {
	    "type": "object",
	    "description": "functional groups to filter",
	    "properties": {
		"smiles": {
		    "type": "array",
		    "description": "smile of the functional group",
		    "items": {
			"type": "string"
		    }
		}
	    }
	}
	},
	"required": ["functional_group"]
    }
    ]
}

