{
    "$schema": "http://json-schema.org/draft-04/schema",
    "description": "Molecular properties to compute",
    "type": "object",
    "properties": {
	"compute_property": {
	    "type": "object",
	    "description": "Molecular properties to compute for a given dataset",
	    "properties": {
		"id": {
		    "type": "integer",
		    "description": "job identifier"
		},
		"depends_on": {
		    "type": "array",
		    "description": "Identifiers of the other tasks that the current job depends on",
		    "contains": {
			"type": "integer"
		    }
		}
	    },
	    "property": {
		"type": "array",
		"description": "Molecular properties",
		"enum": [
		"fingerprint"
		]
	    }
	}
    }
}
